@page
@model TravelGuide.Pages.Info.TransportSchedule.Create

@{
}

<!DOCTYPE html>

<html>
<head>
    <title></title>
</head>
<body>
<div>
    <form method="post">
        <div class="text-danger" asp-validation-summary="ModelOnly"></div>
        <div class="form-group col-md-5 mx-auto">
            <label asp-for="Schedules.From">Начальная остановка</label>
            <input asp-for="Schedules.From" class="form-control"/>
            <span class="text-black-50" asp-validation-for="Schedules.From"></span>
        </div>
        <div class="form-group col-md-5 mx-auto">
            <label asp-for="Schedules.To">Конечная остановка</label>
            <input asp-for="Schedules.To" class="form-control"/>
            <span class="text-black-50" asp-validation-for="Schedules.To"></span>
        </div>
        <div class="form-group col-md-5 mx-auto">
            <label asp-for="Schedules.Time">Время отправления</label>
            <input asp-for="Schedules.Time" class="form-control"/>
            <span class="text-black-50" asp-validation-for="Schedules.Time"></span>
        </div>
        <div class="form-group col-md-5 mx-auto">
            <input type="submit" class="btn btn-success btn-ms btn-block"/>
        </div>
    </form>
</div>
</body>
</html>