@page
@model TravelGuide.Pages.Info.TransportSchedule.Edit

@{
}

<!DOCTYPE html>

<html>
<head>
    <title></title>
</head>
<body>
<div>
    <form method="post">
        <div class="text-danger" asp-validation-summary="ModelOnly"></div>
        <div class="form-group col-md-5 mx-auto">
            <label asp-for="GetSchedule.From">Начальная остановка</label>
            <input asp-for="GetSchedule.From" class="form-control" value="@Model.Schedules.From"/>
            <span class="text-black-50" asp-validation-for="GetSchedule.From"></span>
        </div>
        <div class="form-group col-md-5 mx-auto">
            <label asp-for="GetSchedule.To">Конечная остановка</label>
            <input asp-for="GetSchedule.To" class="form-control" value="@Model.Schedules.To"/>
            <span class="text-black-50" asp-validation-for="GetSchedule.To"></span>
        </div>
        <div class="form-group col-md-5 mx-auto">
            <label asp-for="GetSchedule.Time">Время отправления</label>
            <input asp-for="GetSchedule.Time" class="form-control" value="@Model.Schedules.Time"/>
            <span class="text-black-50" asp-validation-for="GetSchedule.Time"></span>
        </div>
        <div class="form-group col-md-5 mx-auto">
            <input type="submit" class="btn btn-success btn-ms btn-block"/>
        </div>
    </form>
</div>
</body>
</html>